extends layout

block content
  h1= title
  p Welcome to NorthWest #{title} 
  button(type='button',onclick='update()',class='btn btn-primary') Press Me
  div(id="target") text of Display 
  script.
    var client_count = 0;
    function update(){
      client_count +=1;
      document.getElementById("target").innerHTML = "Value now " + client_count;
    }
  a(href='tomato') tomato